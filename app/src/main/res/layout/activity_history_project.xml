<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorAccent"
    tools:context=".activity.HistoryProjectActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/history_project_tool_bar"
            style="@style/ToolbarStyle"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            app:title="Android IDA" />

        <com.xuexiang.xui.widget.searchview.MaterialSearchView
            android:id="@+id/history_project_search_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </FrameLayout>

    <ListView
        android:id="@+id/history_project_list_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include layout="@layout/menu_project_manager_fab" />

</LinearLayout>